<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.morgoth.ServerCoordinator">
    <method name="start">
        <arg type="b" direction="out"/>
    </method>
    <method name="stop">
    </method>
    <property name="status" type="(s)" access="read">
        <annotation name="org.qtproject.QtDBus.QtTypeName" value="morgoth::ServerCoordinator::Status"/>
    </property>
    <property name="playerCount" type="i" access="read"/>
    <property name="maxPlayers" type="i" access="read"/>
    <signal name="statusChanged">
        <arg name="newStatus" type="(s)" direction="out"/>
        <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="morgoth::ServerCoordinator::Status"/>
    </signal>
    <signal name="playerCountChanged">
        <arg name="playerCount" type="i" direction="out"/>
    </signal>
    <signal name="maxPlayersChanged">
        <arg name="maxPlayers" type="i" direction="out"/>
    </signal>
    <method name="flushLogs">
    </method>
  </interface>
</node>
